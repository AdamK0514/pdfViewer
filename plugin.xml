<?xml version="1.0" encoding="UTF-8"?>
    <plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
            id="de.cyberkatze.phonegap.plugin" version="0.0.1">
        <name>PdfViewer</name>
        <description>Cordova pdfviewer Plugin</description>
        <license>Free</license>
        <keywords>cordova,pdf,pdfviewer,viewer,android,open</keywords>
        <js-module src="www/openPDF.js" name="openPDF">
            <clobbers target="window.openPDF" />
        </js-module>
        <platform name="android">
            <config-file target="config.xml" parent="/*">
                <feature name="PdfViewer">
                    <param name="android-package" value="de.cyberkatze.phonegap.plugin.PdfViewer"/>
                </feature>
            </config-file>
        </platform>
    </plugin>